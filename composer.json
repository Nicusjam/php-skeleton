{
  "name": "matthiasmullie/php-skeleton",
  "type": "library",
  "description": "Skeleton for PHP projects",
  "keywords": [
    "php", "skeleton"
  ],
  "homepage": "http://github.com/matthiasmullie/php-skeleton",
  "license": "MIT",
  "authors": [
    {
      "name": "Matthias Mullie",
      "homepage": "http://www.mullie.eu",
      "email": "php-skeleton@mullie.eu",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^5.3|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "friendsofphp/php-cs-fixer": "~1.0",
    "cauditor/analyzer": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "MatthiasMullie\\Skeleton\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MatthiasMullie\\Skeleton\\Tests\\": "tests/"
    }
  }
}
